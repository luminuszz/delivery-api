generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Deliveryman {
    id String @id @default(uuid())
    username String @unique
    password String
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt

    @@map("deliveryman")
}